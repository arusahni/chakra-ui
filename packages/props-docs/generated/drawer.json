{
  "DrawerRoot": {
    "onClose": {
      "type": "() => void",
      "required": true,
      "description": "Callback invoked to close the dialog."
    },
    "open": {
      "type": "boolean",
      "required": true,
      "description": "If `true`, the dialog will be open."
    },
    "allowPinchZoom": {
      "type": "boolean",
      "defaultValue": "false.",
      "required": false,
      "description": "Handle zoom/pinch gestures on iOS devices when scroll locking is enabled."
    },
    "autoFocus": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, the dialog will autofocus the first enabled and interactive\nelement within the `DialogContent`"
    },
    "blockScrollOnMount": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, scrolling will be disabled on the `body` when the dialog opens."
    },
    "closeOnEsc": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, the dialog will close when the `Esc` key is pressed"
    },
    "closeOnOverlayClick": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, the dialog will close when the overlay is clicked"
    },
    "finalFocusRef": {
      "type": "RefObject<FocusableElement>",
      "required": false,
      "description": "The `ref` of element to receive focus when the dialog closes."
    },
    "id": {
      "type": "string",
      "required": false,
      "description": "The `id` of the dialog"
    },
    "initialFocusRef": {
      "type": "RefObject<FocusableElement>",
      "required": false,
      "description": "The `ref` of element to receive focus when the dialog opens."
    },
    "isFullHeight": {
      "type": "boolean",
      "required": false,
      "description": "If `true` and drawer's placement is `top` or `bottom`,\nthe drawer will occupy the viewport height (100vh)"
    },
    "lockFocusAcrossFrames": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "Enables aggressive focus capturing within iframes.\n- If `true`: keep focus in the lock, no matter where lock is active\n- If `false`:  allows focus to move outside of iframe"
    },
    "onCloseComplete": {
      "type": "VoidFunction",
      "required": false,
      "description": "Fires when all exiting nodes have completed animating out"
    },
    "onEsc": {
      "type": "() => void",
      "required": false,
      "description": "Callback fired when the escape key is pressed and focus is within dialog"
    },
    "onOverlayClick": {
      "type": "() => void",
      "required": false,
      "description": "Callback fired when the overlay is clicked."
    },
    "placement": {
      "type": "SlideDirection | LogicalPlacement",
      "defaultValue": "right",
      "required": false,
      "description": "The placement of the drawer"
    },
    "portalProps": {
      "type": "Pick<\n  PortalProps,\n  \"appendToParentPortal\" | \"containerRef\"\n>",
      "required": false,
      "description": "Props to be forwarded to the portal component"
    },
    "preserveScrollBarGap": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, a `padding-right` will be applied to the body element\nthat's equal to the width of the scrollbar.\n\nThis can help prevent some unpleasant flickering effect\nand content adjustment when the dialog opens"
    },
    "returnFocusOnClose": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, the dialog will return focus to the element that triggered it when it closes."
    },
    "role": {
      "type": "\"dialog\" | \"alertdialog\"",
      "required": false,
      "description": "The `role` attribute of the dialog"
    },
    "trapFocus": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `false`, focus lock will be disabled completely.\n\nThis is useful in situations where you still need to interact with\nother surrounding elements.\n\nðŸš¨Warning: We don't recommend doing this because it hurts the\naccessibility of the dialog, based on WAI-ARIA specifications."
    },
    "useInert": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "A11y: If `true`, the siblings of the `dialog` will have `aria-hidden`\nset to `true` so that screen readers can only see the `dialog`.\n\nThis is commonly known as making the other elements **inert**"
    }
  },
  "DrawerContent": {
    "_active": { "type": "SystemStyleObject", "required": false },
    "_activeLink": { "type": "SystemStyleObject", "required": false },
    "_activeStep": { "type": "SystemStyleObject", "required": false },
    "_after": { "type": "SystemStyleObject", "required": false },
    "_autofill": { "type": "SystemStyleObject", "required": false },
    "_before": { "type": "SystemStyleObject", "required": false },
    "_checked": { "type": "SystemStyleObject", "required": false },
    "_closed": { "type": "SystemStyleObject", "required": false },
    "_dark": { "type": "SystemStyleObject", "required": false },
    "_disabled": { "type": "SystemStyleObject", "required": false },
    "_empty": { "type": "SystemStyleObject", "required": false },
    "_even": { "type": "SystemStyleObject", "required": false },
    "_expanded": { "type": "SystemStyleObject", "required": false },
    "_first": { "type": "SystemStyleObject", "required": false },
    "_firstLetter": { "type": "SystemStyleObject", "required": false },
    "_focus": { "type": "SystemStyleObject", "required": false },
    "_focusVisible": { "type": "SystemStyleObject", "required": false },
    "_focusWithin": { "type": "SystemStyleObject", "required": false },
    "_fullScreen": { "type": "SystemStyleObject", "required": false },
    "_grabbed": { "type": "SystemStyleObject", "required": false },
    "_groupActive": { "type": "SystemStyleObject", "required": false },
    "_groupChecked": { "type": "SystemStyleObject", "required": false },
    "_groupClosed": { "type": "SystemStyleObject", "required": false },
    "_groupDisabled": { "type": "SystemStyleObject", "required": false },
    "_groupFocus": { "type": "SystemStyleObject", "required": false },
    "_groupFocusVisible": { "type": "SystemStyleObject", "required": false },
    "_groupFocusWithin": { "type": "SystemStyleObject", "required": false },
    "_groupHover": { "type": "SystemStyleObject", "required": false },
    "_groupInvalid": { "type": "SystemStyleObject", "required": false },
    "_groupOpen": { "type": "SystemStyleObject", "required": false },
    "_hidden": { "type": "SystemStyleObject", "required": false },
    "_highlighted": { "type": "SystemStyleObject", "required": false },
    "_horizontal": { "type": "SystemStyleObject", "required": false },
    "_hover": { "type": "SystemStyleObject", "required": false },
    "_indeterminate": { "type": "SystemStyleObject", "required": false },
    "_invalid": { "type": "SystemStyleObject", "required": false },
    "_last": { "type": "SystemStyleObject", "required": false },
    "_light": { "type": "SystemStyleObject", "required": false },
    "_loading": { "type": "SystemStyleObject", "required": false },
    "_ltr": { "type": "SystemStyleObject", "required": false },
    "_mediaDark": { "type": "SystemStyleObject", "required": false },
    "_mediaReduceMotion": { "type": "SystemStyleObject", "required": false },
    "_notFirst": { "type": "SystemStyleObject", "required": false },
    "_notLast": { "type": "SystemStyleObject", "required": false },
    "_odd": { "type": "SystemStyleObject", "required": false },
    "_open": { "type": "SystemStyleObject", "required": false },
    "_peerActive": { "type": "SystemStyleObject", "required": false },
    "_peerChecked": { "type": "SystemStyleObject", "required": false },
    "_peerDisabled": { "type": "SystemStyleObject", "required": false },
    "_peerFocus": { "type": "SystemStyleObject", "required": false },
    "_peerFocusVisible": { "type": "SystemStyleObject", "required": false },
    "_peerFocusWithin": { "type": "SystemStyleObject", "required": false },
    "_peerHover": { "type": "SystemStyleObject", "required": false },
    "_peerInvalid": { "type": "SystemStyleObject", "required": false },
    "_peerPlaceholderShown": { "type": "SystemStyleObject", "required": false },
    "_placeholder": { "type": "SystemStyleObject", "required": false },
    "_placeholderShown": { "type": "SystemStyleObject", "required": false },
    "_pressed": { "type": "SystemStyleObject", "required": false },
    "_readOnly": { "type": "SystemStyleObject", "required": false },
    "_rtl": { "type": "SystemStyleObject", "required": false },
    "_selected": { "type": "SystemStyleObject", "required": false },
    "_selection": { "type": "SystemStyleObject", "required": false },
    "_valid": { "type": "SystemStyleObject", "required": false },
    "_vertical": { "type": "SystemStyleObject", "required": false },
    "_visited": { "type": "SystemStyleObject", "required": false },
    "motionProps": {
      "type": "HTMLMotionProps<\"section\">",
      "required": false,
      "description": "The custom framer-motion transition to use for the dialog"
    }
  }
}
